{
    "name": "eslint-plugin-backbone",
    "version": "3.0.0",
    "description": "Eslint rules for Backbone.",
    "main": "index.js",
    "scripts": {
        "test": "npm run lint && npm run unit-test && npm run check-coverage",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "transform": "eslint-transforms v9-rule-migration lib/rules",
        "unit-test": "c8 --all --exclude *.config.* node tests",
        "check-coverage": "c8 check-coverage --statement 100 --branch 100 --function 100 --lines 100"
    },
    "files": [
        "lib",
        "index.js"
    ],
    "devDependencies": {
        "@stylistic/eslint-plugin-js": "^2.9.0",
        "eslint": "^9.12.0",
        "eslint-plugin-jsdoc": "^50.3.1",
        "c8": "^10.1.2"
    },
    "peerDependencies": {
        "eslint": ">=9.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/ilyavolodin/eslint-plugin-backbone.git"
    },
    "author": "Ilya Volodin <ivolodin@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ilyavolodin/eslint-plugin-backbone/issues"
    },
    "homepage": "https://github.com/ilyavolodin/eslint-plugin-backbone",
    "keywords": [
        "eslint-plugin",
        "eslintplugin",
        "backbone",
        "eslint"
    ],
    "dependencies": {
        "eslint-transforms": "^3.0.0"
    }
}
